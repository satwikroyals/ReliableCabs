                                                {  <p>&nbsp&nbsp $ 0</p>}
                                                else
                                                {  <p>&nbsp&nbsp $@Convert.ToDecimal(@i.TotEftposAmt).ToString("#.##")</p>} </td>
                                      
                                         <td style="text-align:center;border-left-style:hidden;">@if (i.TotalEftposAmt == 0 || Convert.ToString(i.TotalEftposAmt).Trim().Length == 0 || i.TotalEftposAmt <= 0)
                                                {  <p> $ 0</p>}
                                                else
                                                {  <p>$@Convert.ToDecimal(@i.TotalEftposAmt).ToString("#.##")</p>}                                             
                                           </td>

                                    </tr>
     }
                               </tbody>
                            </table>
							
                        </div>
				  </div>  




